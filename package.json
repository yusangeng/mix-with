{
  "name": "mix-with",
  "version": "3.2.4",
  "description": "A type-safe mixin helper function.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./esm/index.js"
  },
  "scripts": {
    "lint": "eslint --fix ./src && prettier --write ./src",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha",
    "coverage": "nyc report --reporter=lcov",
    "esm": "rm -rf ./esm && rm -rf types && tsc -p tsconfig.esm.json",
    "cjs": "rm -rf ./cjs && tsc -p tsconfig.cjs.json",
    "build": "npm run esm && npm run cjs",
    "prepublishOnly": "npm run test && npm run build",
    "ci": "npm run lint && npm run build && npm run test && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/mix-with.git"
  },
  "keywords": [
    "mixin"
  ],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
